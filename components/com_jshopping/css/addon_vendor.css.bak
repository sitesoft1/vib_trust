}
a.toolbar .icon-32-unpublish{
    background: url("../images/icon-32-unpublish.png") no-repeat center top;
}
a.toolbar .icon-32-delete{
    background: url("../images/icon-32-delete.png") no-repeat center top;
}
a.toolbar .icon-32-back{
    background: url("../images/icon-32-back.png") no-repeat center top;
